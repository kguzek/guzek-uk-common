{
  "name": "guzek-uk-common",
  "version": "3.2.2",
  "description": "1 repository of common libraries and utilities for the Guzek UK Website. ",
  "author": "Konrad Guzek",
  "homepage": "https://github.com/kguzek/guzek-uk-common#readme",
  "license": "AGPL-3.0-only",
  "engines": {
    "node": "^22.3.0"
  },
  "scripts": {
    "clean": "rm -rf dist guzek-uk-common-*.tgz",
    "build": "npm i; npm run clean; tsc && npm pack",
    "release": "npm publish --access public",
    "bump": "for i in ../guzek-uk-{api,*-server}; do cd $i; npm run up; git add package-lock.json; git commit -m 'bump guzek-uk-common'; git push; done",
    "deploy": "npm run build && npm run release && npm run bump"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kguzek/guzek-uk-common.git"
  },
  "bugs": {
    "url": "https://github.com/kguzek/guzek-uk-common/issues"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/*/index.d.ts"
      ]
    }
  },
  "exports": {
    "./lib/*": "./dist/lib/*.js",
    "./middleware": "./dist/middleware/index.js",
    "./enums": "./dist/enums.js",
    "./models": "./dist/models.js"
  },
  "files": [
    "dist/*",
    "LICENSE",
    "README.md"
  ],
  "dependencies": {
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "ip-address": "^10.0.1",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0",
    "method-override": "^3.0.0",
    "mysql2": "^3.11.5",
    "sequelize": "^6.37.5",
    "sequelize-typescript": "^2.1.6",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/method-override": "^3.0.0",
    "@types/node": "^22.10.2",
    "@types/sequelize": "^4.28.20",
    "typescript": "^5.7.2"
  }
}
