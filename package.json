{
  "name": "guzek-uk-common",
  "version": "2.0.11",
  "description": "1 repository of common libraries and utilities for the Guzek UK Website. ",
  "author": "Konrad Guzek",
  "homepage": "https://github.com/kguzek/guzek-uk-common#readme",
  "license": "AGPL-3.0-only",
  "engines": {
    "node": ">=22.3.0"
  },
  "scripts": {
    "clean": "rm -r dist guzek-uk-common-*.tgz",
    "build": "npm run clean; tsc && npm pack",
    "release": "npm publish --access public",
    "deploy": "npm run build && npm run release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kguzek/guzek-uk-common.git"
  },
  "bugs": {
    "url": "https://github.com/kguzek/guzek-uk-common/issues"
  },
  "typesVersions": {
    "*": {
      "middleware": [
        "dist/middleware/index.d.ts"
      ],
      "logger": [
        "dist/logger.d.ts"
      ],
      "maths": [
        "dist/maths.d.ts"
      ],
      "models": [
        "dist/models.d.ts"
      ],
      "sequelize": [
        "dist/sequelize.d.ts"
      ],
      "server": [
        "dist/server.d.ts"
      ],
      "setup": [
        "dist/setup.d.ts"
      ],
      "util": [
        "dist/util.d.ts"
      ]
    }
  },
  "exports": {
    "./middleware": "./dist/middleware/index.js",
    "./logger": "./dist/logger.js",
    "./maths": "./dist/maths.js",
    "./models": "./dist/models.js",
    "./sequelize": "./dist/sequelize.js",
    "./server": "./dist/server.js",
    "./setup": "./dist/setup.js",
    "./util": "./dist/util.js"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "dependencies": {
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "fs": "^0.0.1-security",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0",
    "mysql2": "^3.11.5",
    "sequelize": "^6.37.5",
    "sequelize-typescript": "^2.1.6",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.10.2",
    "@types/sequelize": "^4.28.20",
    "typescript": "^5.7.2"
  }
}
