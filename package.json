{
  "name": "guzek-uk-common",
  "version": "1.1.2",
  "description": "A repository of common libraries and utilities for the Guzek UK Website. ",
  "engines": {
    "node": "^22.12.0"
  },
  "scripts": {
    "build": "tsc && npm pack",
    "publish": "npm publish --access public",
    "deploy": "npm run build && npm run publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kguzek/guzek-uk-common.git"
  },
  "author": "Konrad Guzek",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/kguzek/guzek-uk-common/issues"
  },
  "homepage": "https://github.com/kguzek/guzek-uk-common#readme",
  "typesVersions": {
    "*": {
      "middleware": [
        "dist/middleware/index.d.ts"
      ],
      "logger": [
        "dist/logger.d.ts"
      ],
      "maths": [
        "dist/maths.d.ts"
      ],
      "models": [
        "dist/models.d.ts"
      ],
      "sequelize": [
        "dist/sequelize.d.ts"
      ],
      "util": [
        "dist/util.d.ts"
      ]
    }
  },
  "exports": {
    "./middleware": "./dist/middleware/index.js",
    "./logger": "./dist/logger.js",
    "./maths": "./dist/maths.js",
    "./models": "./dist/models.js",
    "./sequelize": "./dist/sequelize.js",
    "./util": "./dist/util.js"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "dependencies": {
    "@types/jsonwebtoken": "^9.0.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "fs": "^0.0.1-security",
    "jsonwebtoken": "^9.0.2",
    "sequelize": "^6.37.5",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.2",
    "@types/sequelize": "^4.28.20",
    "typescript": "^5.7.2"
  }
